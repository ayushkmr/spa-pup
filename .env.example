# Docker Compose Environment Variables
# Copy this file to .env for using latest images or .env.dev/.env.prod for specific environments

# Node Environment
NODE_ENV=development # or production

# Backend Image
# Use one of the following options:
BACKEND_IMAGE=ghcr.io/ayushkmr/spa-pup-backend:latest
# BACKEND_IMAGE=ghcr.io/ayushkmr/spa-pup-backend:dev
# BACKEND_IMAGE=ghcr.io/ayushkmr/spa-pup-backend:prod
# BACKEND_IMAGE=ghcr.io/ayushkmr/spa-pup-backend:sha-d2da96b

# Frontend Image
# Use one of the following options:
FRONTEND_IMAGE=ghcr.io/ayushkmr/spa-pup-frontend:latest
# FRONTEND_IMAGE=ghcr.io/ayushkmr/spa-pup-frontend:dev
# FRONTEND_IMAGE=ghcr.io/ayushkmr/spa-pup-frontend:prod
# FRONTEND_IMAGE=ghcr.io/ayushkmr/spa-pup-frontend:sha-d2da96b

# Database Configuration
DB_USERNAME=puppy
DB_PASSWORD=puppy_spa
DB_NAME=puppy_spa
DB_PORT=5432
DATABASE_URL=postgresql://puppy:puppy_spa@postgres:5432/puppy_spa?schema=public

# pgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=admin@puppyspa.com
PGADMIN_DEFAULT_PASSWORD=admin
PGADMIN_PORT=5050

# API Configuration
API_PORT=3005
CORS_ORIGIN=true
HOST=0.0.0.0

# Frontend Configuration
NEXT_PUBLIC_API_URL=/api/ 